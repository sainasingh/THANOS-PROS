

#!/bin/bash


_python_() {
    python3${pVer%.*} -c "$1"
}

_THANOSBOT_MAIN() {
    $(_python_ 'from git import Repo
import sys

_UPSTREAM_ = "https://github.com/rishabh87yhtts/THANOS-PRO"
_BRANCH_ = "master"

repo = Repo.init()
origin = repo.create_remote("temponame", _UPSTREAM_)
origin.fetch()
repo.create_head(_BRANCH_, origin.refs[_BRANCH_])
repo.heads[_BRANCH_].checkout(True) ')
}

_THANOS_repo () {
    local hrepo
    hrepo=`echo "aHR0cHM6Ly9naXRodWIuY29tL3Jpc2hhYmg4N3lodHRzL1RIQU5PUy1QUk8=" | base64 -d`
    echo "$rlink"
}

_THANOS_zip () {
    echo "aHR0cHM6Ly9naXRodWIuY29tL3Jpc2hhYmg4N3lodHRzL1RIQU5PUy1QUk8vYXJjaGl2ZS9tYXN0ZXIuemlw" | base64 -d
}

_THANOSBOT_PLUG() {
    local hrepo=$(_THANOS_repo)
    $(_python_ 'from git import Repo
import sys
_UPSTREAM_ = "'$hrepo'"
_BRANCH_ = "master"
repo = Repo.init()
origin = repo.create_remote("temponame", _UPSTREAM_)
origin.fetch()
repo.create_head(_BRANCH_, origin.refs[_BRANCH_])
repo.heads[_BRANCH_].checkout(True) ')
}

_THANOS_bot () {
    local zippath
    zippath="THANOS-PRO.zip"
    echo "  ğŸ’• Downloading ï¼´ï¼¨Î›ï¼®â™¢ï¼³ï¼ï¼°ï¼²â™¢ Source Code... ğŸ’•"
    wget -q $(_get_repolink) -O "$zippath"
    echo "  ğŸ’“Unpacking Data...ğŸ’“"
    THANOS-PROPATH=$(zipinfo -1 "$zippath" | grep -v "/.");
    unzip -qq "$zippath"
    echo " ğŸ’” Download Complete Now Installing ğŸ’”"
    echo "  âš¡ INSTALLING ï¼´ï¼¨Î›ï¼®â™¢ï¼³ï¼ï¼°ï¼²â™¢ âš¡"
    rm -rf "$zippath"
    sleep 5
    cd $THANOS-PROPATH
    echo "    âš¡Starting ï¼´ï¼¨Î›ï¼®â™¢ï¼³ï¼ï¼°ï¼²â™¢âš¡     "
    echo " ğŸ’ NOW WAIT 3 MIN INSTALLING ALL THANOS PLUGIN ğŸ’"

    python3 ../setup/updater.py ../requirements.txt requirements.txt
    python3 -m THANOSPRO
}

_THANOS_bot
